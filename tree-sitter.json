{
  "grammars": [
    {
      "name": "antlers",
      "camelcase": "Antlers",
      "scope": "source.antlers",
      "path": ".",
      "file-types": [
        "antlers.html",
        "antlers.php"
      ],
      "injection-regex": "^antlers$",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for Statamic Antlers template language",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-antlers"
    }
  },
  "bindings": {
    "c": true,
    "node": true,
    "rust": true
  }
}
